{{parseJson request.body 'json'}}
{
  "objectType": "WORKER",
  "id" : "{{randomValue type='UUID'}}",
  "identity" : "{{json.identity}}",
  "taskQueue" : "{{json.taskQueue}}",
  "workflowTypes" : [
    {{#each json.workflowTypes}}
      "{{this}}"{{#unless @last}},{{/unless}}
    {{/each}}
  ],
  "activityTypes" :[
    {{#each json.activityTypes}}
      "{{this}}"{{#unless @last}},{{/unless}}
    {{/each}}
  ],
  "hostname" : "{{json.hostname}}",
  "ip" : "{{json.ip}}",
  "createdBy": "{{randomValue type='UUID'}}",
  "createdAt": "{{now}}",
  "lastModifiedBy": "{{randomValue type='UUID'}}",
  "lastModifiedAt": "{{now}}"
}
